<% provide(:title, "Sign in") %> <h1>Sign in</h1>
<%= bootstrap_form_for(:session, url: sessions_path) do |f| %>
<%= f.text_field :email %>
<%= f.password_field :password %>
<%= f.submit "Sign in", class: "btn btn-large btn-primary" %>
<% end %>

<p>
	New user? <%= link_to "Sign up now!", signup_path %>
</p>
